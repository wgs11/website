

<table>
  <% @users.each do |user| %>
    <tr>
      <td><%=user.display_name%></td>
          <% if Rails.env.development? %>
            <td class="list_actions">
              <%= link_to 'Show', user %> </br>
              <%= link_to 'Edit', edit_user_path(user) %></br>
              <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
            </td>
          <% end %>
          </tr>
  <% end %>
</table>